{
  "name": "kerang-ajaib-bot",
  "version": "1.0.0",
  "private": true,
  "description": "Sebuah bot discord yang bisa menjawab berbagai macam pertanyaan untukmu",
  "license": "MIT",
  "author": "Syukur M.",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "pnpm generate && pnpm build:tsc",
    "build:tsc": "tsc",
    "dev": "cross-env NODE_ENV=development ts-node -T src/index.ts",
    "generate": "prisma generate",
    "start": "cross-env NODE_ENV=production node --experimental-specifier-resolution=node dist/index.js",
    "typecheck": "tsc --noEmit",
    "watch": "nodemon --watch src -e ts --exec pnpm dev"
  },
  "dependencies": {
    "@discordx/importer": "^1.1.10",
    "@discordx/utilities": "^5.1.0",
    "@prisma/client": "^4.4.0",
    "consola": "^2.15.3",
    "cross-env": "^7.0.3",
    "discord.js": "^14.5.0",
    "discordx": "^11.4.0",
    "dotenv": "^16.0.3",
    "envsafe": "^2.0.3",
    "reflect-metadata": "^0.1.13",
    "tslib": "^2.4.0",
    "typedi": "^0.10.0"
  },
  "devDependencies": {
    "@types/node": "^18.8.3",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "eslint": "^8.24.0",
    "eslint-config-adjunct": "^4.11.2",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-auto": "^0.7.3",
    "eslint-plugin-array-func": "^3.1.7",
    "eslint-plugin-etc": "^2.0.2",
    "eslint-plugin-github": "^4.3.7",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-markdown": "^3.0.0",
    "eslint-plugin-no-constructor-bind": "^2.0.4",
    "eslint-plugin-no-secrets": "^0.8.9",
    "eslint-plugin-no-unsanitized": "^4.0.1",
    "eslint-plugin-no-use-extend-native": "^0.5.0",
    "eslint-plugin-optimize-regex": "^1.2.1",
    "eslint-plugin-promise": "^6.0.1",
    "eslint-plugin-regexp": "^1.9.0",
    "eslint-plugin-scanjs-rules": "^0.2.1",
    "eslint-plugin-security": "^1.5.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-sonarjs": "^0.15.0",
    "eslint-plugin-switch-case": "^1.1.2",
    "eslint-plugin-unicorn": "^44.0.2",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "prisma": "^4.4.0",
    "ts-node": "^10.9.1",
    "typescript": "4.8.4"
  }
}
